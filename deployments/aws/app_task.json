[
  {
    "name": "app",
    "image": "418124467834.dkr.ecr.eu-central-1.amazonaws.com/kuffel/ex_app:${docker_tag}",
    "cpu": 256,
    "memory": 512,
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "portMappings": [
      {
        "containerPort": 4000,
        "hostPort": 4000,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "entryPoint": [],
    "command": [],
    "environment": [],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "ex-app-logs",
        "awslogs-region": "eu-central-1",
        "awslogs-stream-prefix": "app"
      }
    }
  }
]
